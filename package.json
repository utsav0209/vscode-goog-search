{
  "name": "goog-search",
  "displayName": "Google Search",
  "description": "Open default browser windows to search selected text in editor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:goog-search.editor",
    "onCommand:goog-search.terminal"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "goog-search.editor",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "goog-search.editor",
        "key": "Ctrl+Shift+g",
        "when": "editorHasSelection"
      },
      {
        "command": "goog-search.terminal",
        "key": "Ctrl+Shift+g",
        "when": "terminalTextSelected"
      }
    ],
    "commands": [
      {
        "command": "goog-search.editor",
        "title": "Search with Google"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.58.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
